{
  "manifest_version": 2,
  "name": "CORS Allow",
  "version": "1.0",
  "description": "Allow CORS for specified URL.",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "CORS Allow"
  }
}
