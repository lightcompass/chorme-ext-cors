{
  "manifest_version": 3,
  "name": "CORS Allow",
  "version": "1.0",
  "description": "Allow CORS for specified URL.",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "CORS Allow"
  }
}
